<?xml version="1.0" encoding="utf-8"?>
<databaseChangeLog
    xmlns="http://www.liquibase.org/xml/ns/dbchangelog"
    xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
    xsi:schemaLocation="http://www.liquibase.org/xml/ns/dbchangelog http://www.liquibase.org/xml/ns/dbchangelog/dbchangelog-3.6.xsd">
    <!--
        Added the constraints for entity Tasker.
    -->
    <changeSet id="20191128073438-2" author="jhipster">
        
        <addForeignKeyConstraint baseColumnNames="user_id"
                                 baseTableName="tasker"
                                 constraintName="fk_tasker_user_id"
                                 referencedColumnNames="id"
                                 referencedTableName="user_information"/>

        <addForeignKeyConstraint baseColumnNames="tasker_id"
                                 baseTableName="tasker_tasker_categories"
                                 constraintName="fk_tasker_tasker_categories_tasker_id"
                                 referencedColumnNames="id"
                                 referencedTableName="tasker"/>

        <addForeignKeyConstraint baseColumnNames="tasker_categories_id"
                                 baseTableName="tasker_tasker_categories"
                                 constraintName="fk_tasker_tasker_categories_tasker_categories_id"
                                 referencedColumnNames="id"
                                 referencedTableName="tasker_category"/>
        
    </changeSet>
</databaseChangeLog>
